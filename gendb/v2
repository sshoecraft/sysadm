SELECT ai.Name, si.Environment, si.Criticality, vc.[OS Name], vc.[IP Address], va.Manufacturer, va.Model, va.[Serial Number], bi.[Backup Method], usi.Notes as [System Notes], va.Status, uss.[App Support], uss.[Tech Support], usi.[warranty type], usi.[warranty information], usi.start, usi.[End], os.[Last Boot Up Time], id._ResourceGuid FROM vRM_Asset_Item ai LEFT JOIN vAsset va ON ai.Guid = va._ResourceGuid LEFT JOIN _vUSON_ComputerType ct ON ct.Guid = ai.Guid LEFT  JOIN Inv_AeX_AC_Identification id ON ai.Guid = id._ResourceGuid LEFT  JOIN vComputer vc ON ai.Guid = vc.Guid LEFT  JOIN  Inv_USON_System_Info si ON ai.Guid = si._ResourceGuid LEFT  JOIN Inv_USON_MPN mpn ON ai.Guid = mpn._ResourceGuid LEFT  JOIN Inv_USON_Backup_Info bi ON ai.Guid = bi._ResourceGuid LEFT  JOIN Inv_USON_System_Info usi ON ai.Guid = usi._ResourceGuid LEFT  JOIN Inv_USON_System_Support uss ON ai.Guid = uss._ResourceGuid LEFT  JOIN Inv_OS_Operating_System os ON os._ResourceGuid = ID._ResourceGuid WHERE va.[Asset Type] IN ('Computer', 'Virtual Machine');
